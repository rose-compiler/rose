  // DQ (8/30/2009): Added support for SgNamespaceAliasDeclarationStatement
     SgNode* declaration = NULL;
     if (p_isAlias == true)
        {
          declaration = get_aliasDeclaration();
        }
       else
        {
          declaration = get_declaration();
        }

  // return get_declaration();
     return declaration;
   }

