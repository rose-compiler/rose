SgNamespaceSymbol::post_construction_initialization()
   {
  // DQ (8/30/2009): Added support for namespace alias in the SgNamespaceSymbol

     ROSE_ASSERT(p_declaration != NULL || p_aliasDeclaration != NULL);
     ROSE_ASSERT(p_declaration == NULL || p_aliasDeclaration == NULL);

