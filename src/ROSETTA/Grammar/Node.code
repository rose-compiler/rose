// This is all documented in Rva.C since doxygen can't pick of documentation from this file.
class ROSE_DLL_API rose_rva_t {
        public:
                rose_rva_t();
                rose_rva_t(const rose_rva_t&);
                rose_rva_t operator=(const rose_rva_t&);
                rose_rva_t(rose_addr_t rva, SgAsmGenericSection *section=NULL); //implicit
                static rose_rva_t section_relative(SgAsmGenericSection *section, rose_addr_t offset);
                bool is_bound() const;
                rose_addr_t get_rva() const;
                rose_rva_t& set_rva(rose_addr_t rva);
                SgAsmGenericSection *get_section() const;
                rose_rva_t& set_section(SgAsmGenericSection *section);
                rose_rva_t& bind(SgAsmGenericHeader*);
                rose_addr_t get_va() const;
                rose_addr_t get_rel() const;
                rose_addr_t get_rel(SgAsmGenericSection*);
                void increment(rose_addr_t amount);
                std::string to_string() const;
        private:
                rose_addr_t addr;             /* virtual address (relative to section if section is not null) */
                SgAsmGenericSection *section; /* rva is relative to header base_va if section is null */

#ifdef ROSE_HAVE_BOOST_SERIALIZATION_LIB
        private:
                friend class boost::serialization::access;

                template<class S>
                void serialize(S &s, const unsigned /*version*/) {
                    s & BOOST_SERIALIZATION_NVP(addr);
                    s & BOOST_SERIALIZATION_NVP(section);
                }
#endif
};
#endif

