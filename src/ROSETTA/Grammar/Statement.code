SgStatement::replace_statement(SgStatement* target, const SgStatementPtrList& newstmtList)
   {
     SgBasicBlock* tmp = new SgBasicBlock();
     tmp->get_statements() = newstmtList;
     bool r = replace_child( target, tmp, true);
     assert(r);
   }

