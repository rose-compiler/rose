void
unset_system_bitflag(SgAttributePtrList& pattr, unsigned long int bit)
   {
     SgAttributePtrList::iterator p=pattr.begin();  
     if(p==pattr.end())
        {
          SgBitAttribute* newAttribute = new SgBitAttribute("SYS_BITFLAG");
          assert(newAttribute != NULL);
