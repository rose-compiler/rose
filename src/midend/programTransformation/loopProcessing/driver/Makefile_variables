	$(mptlpDriverPath)/CMakeLists.txt

mptlpDriver_cleanLocal=\
	rm -rf \
		$(mptlpDriverPath)/cxx_templates \
		$(mptlpDriverPath)/ii_files \
		$(mptlpDriverPath)/ti_files \
		$(mptlpDriverPath)/Templates.DB

mptlpDriver_distCleanLocal=\
	rm -rf \
		$(mptlpDriverPath)/Templates.DB

