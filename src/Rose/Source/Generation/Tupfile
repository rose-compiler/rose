include_rules

run $(librose_compile)				\
    API.C					\
    Driver.C					\
    Factory.C					\
    SymbolScanner.C

run $(public_header) -o include/rose/Rose/Source/Generation	\
    API.h							\
    API.txx							\
    Driver.h							\
    Factory.h							\
    Object.h							\
    SymbolScanner.h						\
    SymbolScanner.txx

run $(public_header) -o include/rose/Rose/Source/Generation/factory	\
    factory/factory.txx							\
    factory/namespaces.txx						\
    factory/classes.txx							\
    factory/typedefs.txx						\
    factory/variables.txx						\
    factory/functions.txx
